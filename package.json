{
  "name": "orange-app",
  "version": "0.1.0",
  "private": true,
  "description": "Front app for Saint-Petersburg University of Telecommunications programming olympiad system",
  "author": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "tslint --fix -c ./tslint.json ./src/**/*.ts",
    "dev": "yarn serve",
    "generate-api-types": "yarn run apollo codegen:generate --localSchemaFile=graphql.schema.json --target=typescript --includes=src/**/*.graphql --tagName=gql --globalTypesFile=src/api/database/global-types.ts types",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "update-schema": "yarn run apollo schema:download --endpoint=http://localhost:8765/graphql graphql.schema.json"
  },
  "dependencies": {
    "apollo": "^2.16.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-state": "^0.4.2",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "deep-map": "^2.0.0",
    "define-lazy-prop": "^2.0.0",
    "es6-weak-map": "^2.0.2",
    "graphql": "^14.4.2",
    "lodash.throttle": "^4.1.1",
    "p-debounce": "^2.1.0",
    "p-lazy": "^3.0.0",
    "prismjs": "^1.16.0",
    "rxjs": "^6.5.2",
    "v-click-outside": "^2.1.1",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-i18n": "^1.11.0",
    "vuex-module-decorators": "^0.3.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/jest": "^23.1.4",
    "@types/vuex-i18n": "^1.10.1",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-apollo": "^0.21.0",
    "vue-template-compiler": "^2.6.10"
  },
  "bugs": {
    "url": "https://github.com/LeoVS09/orange-app/issues"
  },
  "homepage": "https://github.com/LeoVS09/orange-app#readme",
  "license": "GPL-3.0",
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoVS09/orange-app.git"
  }
}
