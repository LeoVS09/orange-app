{
  "name": "orange-app",
  "version": "0.1.0",
  "private": true,
  "description": "Front app for Saint-Petersburg University of Telecommunications programming olympiad system",
  "author": "LeoVS010@gmail.com",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "concurrently -k -n e2e,mock \"yarn test:e2e-dev\" \"yarn mock\"",
    "test:e2e-dev": "vue-cli-service test:e2e --mode development",
    "lint": "vue-cli-service lint --fix",
    "apollo": "vue-cli-service apollo:dev --host='0.0.0.0'",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "apollo:start": "vue-cli-service apollo:start",
    "dev": "concurrently -k -r -n serve,mock \"yarn serve\" \"yarn mock\"",
    "generate-api-types": "yarn run apollo codegen:generate --localSchemaFile=graphql.schema.json --target=typescript --includes=src/**/*.graphql --tagName=gql --globalTypesFile=src/api/database/global-types.ts types",
    "mock": "yarn run mock:server",
    "mock:once": "yarn run run-graphql-api",
    "mock:server": "yarn run apollo:start",
    "update-schema": "yarn run apollo schema:download --endpoint=http://localhost:8765/graphql graphql.schema.json",
    "clear:cache": "rm -rf /node_modules/.cache"
  },
  "dependencies": {
    "apollo": "^2.16.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-state": "^0.4.2",
    "axios": "^0.19.0",
    "core-js": "^3.6.4",
    "crypto-js": "^3.1.9-1",
    "deep-map": "^2.0.0",
    "define-lazy-prop": "^2.0.0",
    "es6-weak-map": "^2.0.2",
    "gql-query-builder": "^3.3.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.2.1",
    "immer": "^3.1.3",
    "lodash.throttle": "^4.1.1",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "object-hash": "^2.0.3",
    "p-debounce": "^2.1.0",
    "p-lazy": "^3.0.0",
    "prismjs": "^1.16.0",
    "rxjs": "^6.5.2",
    "shortid": "^2.2.8",
    "subscriptions-transport-ws": "^0.9.16",
    "v-click-outside": "^2.1.1",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-class-component": "^7.0.2",
    "vue-cli-plugin-apollo": "~0.21.3",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-i18n": "^1.11.0",
    "vuex-module-decorators": "^0.3.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.19",
    "@types/object-hash": "^1.3.1",
    "@types/vuex-i18n": "^1.10.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-e2e-cypress": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-plugin-typescript": "~4.2.3",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-service": "~4.2.3",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "casual": "^1.6.2",
    "concurrently": "^5.1.0",
    "cypress": "3.8.3",
    "eslint": "^6.7.2",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "flush-promises": "^1.0.2",
    "lint-staged": "^8.1.5",
    "memoize-fs": "^1.4.1",
    "nock": "^12.0.2",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.9.0",
    "prettier": "1.19.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^4.0.2",
    "vue-template-compiler": "2.6.10"
  },
  "bugs": {
    "url": "https://github.com/LeoVS09/orange-app/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "yarn test:unit"
  },
  "homepage": "https://github.com/LeoVS09/orange-app#readme",
  "license": "GPL-3.0",
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "git add"
    ],
    "*.vue": [
      "yarn lint",
      "git add"
    ],
    "*.{js,vue,ts}": [
