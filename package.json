{
   "name": "orange-app",
   "version": "0.1.0",
   "description": "Front app for Saint-Petersburg University of Telecommunications programming olympiad system",
   "main": "index.js",
   "scripts": {
      "dev": "node build/dev-server.js",
      "start": "node build/dev-server.js",
      "build": "node build/build.js",
      "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
      "e2e": "node test/e2e/runner.js",
      "test": "npm run unit && npm run e2e",
      "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
      "server": "node server/index.js",
      "update-schema": "yarn run apollo schema:download --endpoint=http://localhost:8765/graphql graphql.schema.json",
      "generate-api-types": "yarn run apollo codegen:generate --localSchemaFile=graphql.schema.json --target=typescript --includes=src/**/*.graphql --tagName=gql --globalTypesFile=src/api/graphql/global-types.ts types"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/LeoVS09/orange-app.git"
   },
   "author": "",
   "license": "GPL-3.0",
   "bugs": {
      "url": "https://github.com/LeoVS09/orange-app/issues"
   },
   "homepage": "https://github.com/LeoVS09/orange-app#readme",
